{
  "expo": {
    "name": "RoomRadar",
    "slug": "furnishsnap",
    "version": "1.0.3",
    "runtimeVersion": "1.0.3",
    "owner": "itsedaxe",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#007AFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.furnishsnap.app",
      "buildNumber": "6",
      "infoPlist": {
        "NSCameraUsageDescription": "RoomRadar needs camera access to scan and identify furniture in your photos.",
        "NSPhotoLibraryUsageDescription": "RoomRadar needs photo library access to select images of furniture to scan.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow RoomRadar to access your camera to scan furniture."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow RoomRadar to access your photos to select furniture images."
        }
      ]
    ],
    "extra": {
      "apiUrl": "https://furnishsnap-api-production.up.railway.app",
      "eas": {
        "projectId": "1ed7d88f-51a0-450b-b875-4398c60ab1df"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/1ed7d88f-51a0-450b-b875-4398c60ab1df"
    },
    "android": {
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    }
  }
}
